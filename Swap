#include <iostream>
using namespace std; 
int main()
{ 
  //Write a program to swap two number without using third variable
  int num1,num2,temp; 
  cout << "Enter first number=";
  cin >> num1; 
  cout << "Enter second number=";
  cin >> num2;
  temp=num1;
  num1=num2;
  num2=temp;
  cout << "Swapped numbers:\n";
  cout << "first number=" << num1 << endl;
  cout << "second number=" << num2;
 }
